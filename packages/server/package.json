{
    "name": "@sommelier/data-service",
    "version": "1.0.0",
    "license": "Apache-2.0",
    "licenses": [
        {
            "type": "Apache-2.0",
            "url": "http://www.apache.org/licenses/LICENSE-2.0"
        }
    ],
    "description": "Data tools for DeFi LPs",
    "engines": {
        "node": ">=14 <=15"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "start": "NODE_PATH=dist/ node dist/www.js",
        "clean": "rimraf dist *.tsbuildinfo",
        "build": "ttsc -p tsconfig.build.json && yarn build:copy",
        "build:watch": "ts-node -C build.ts && tsc -w",
        "build:copy": "cp -R src/docs/ dist/docs/",
        "dev": "nodemon src/www.ts | pino-pretty",
        "dev:debug": "nodemon --exec \"node -r ts-node/register --inspect-brk\" src/www.ts | pino-pretty",
        "lint": "eslint --ext js,ts src test",
        "lint:fix": "eslint --ext js,ts --fix src test",
        "//": "Must --forceExit due to supertest not closing https://github.com/visionmedia/supertest/issues/437",
        "test": "NODE_ENV=test jest --forceExit",
        "redis": "ts-node redis",
        "pre-commit": "yarn lint-staged"
    },
    "dependencies": {
        "@alch/alchemy-web3": "^0.1.18",
        "@apollo/client": "^3.3.6",
        "@sommelier/shared-types": "^1.0.0",
        "bignumber.js": "^9.0.1",
        "body-parser": "^1.19.0",
        "bufferutil": "^4.0.2",
        "celebrate": "^14.0.0",
        "config": "^3.3.6",
        "cookie-parser": "^1.4.5",
        "cross-fetch": "^3.0.6",
        "date-fns": "^2.21.1",
        "dotenv": "^8.2.0",
        "ethers": "^5.4.1",
        "express": "^4.17.1",
        "express-openapi-validator": "^4.10.2",
        "express-redis-cache": "^1.1.3",
        "firebase-admin": "^9.8.0",
        "graphql": "^15.5.0",
        "ioredis": "^4.22.0",
        "memory-cache": "^0.2.0",
        "mixpanel": "^0.13.0",
        "morgan": "^1.10.0",
        "node-telegram-bot-api": "^0.51.0",
        "pino": "^6.11.3",
        "swagger-ui-express": "^4.1.6",
        "utf-8-validate": "^5.0.3",
        "web3": "^1.3.1",
        "ws": "^7.4.1",
        "yamljs": "^0.3.0"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.12.11",
        "@babel/preset-typescript": "^7.12.7",
        "@types/chai": "^4.2.14",
        "@types/config": "^0.0.38",
        "@types/cookie-parser": "^1.4.2",
        "@types/express": "^4.17.11",
        "@types/ioredis": "^4.19.4",
        "@types/memory-cache": "^0.2.1",
        "@types/mime-types": "^2.1.0",
        "@types/mocha": "^8.0.3",
        "@types/morgan": "^1.9.2",
        "@types/multer": "^1.4.5",
        "@types/node": "^15.3.0",
        "@types/node-fetch": "^2.5.7",
        "@types/node-telegram-bot-api": "^0.51.0",
        "@types/pino": "^6.3.8",
        "@types/pino-http": "^5.0.6",
        "@types/shelljs": "^0.8.8",
        "@types/sinon": "^10.0.0",
        "@types/swagger-ui-express": "^4.1.2",
        "@types/ws": "^7.4.0",
        "@types/yamljs": "^0.2.31",
        "@typescript-eslint/eslint-plugin": "^4.6.1",
        "@typescript-eslint/parser": "^4.6.1",
        "chai": "^4.2.0",
        "eslint": "^7.13.0",
        "eslint-config-prettier": "^6.15.0",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-unicorn": "^30.0.0",
        "jest": "^26.6.3",
        "mocha": "^8.2.1",
        "nodemon": "^2.0.6",
        "pino-pretty": "^4.3.0",
        "prettier": "^2.1.2",
        "shelljs": "^0.8.4",
        "supertest": "^6.1.3",
        "ts-node": "^9.0.0",
        "tsconfig-paths": "^3.9.0"
    },
    "lint-staged": {
        "**/*.{js,ts,tsx}": [
            "prettier --write",
            "eslint --cache --fix"
        ]
    },
    "volta": {
        "node": "14.17.3"
    }
}
