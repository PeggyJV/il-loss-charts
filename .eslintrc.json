{
    "env": {
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "prettier",
        "prettier/react"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "project": "packages/tsconfig.settings.json"
    },
    "rules": {
        "prefer-const": "error",
        "no-unused-vars": "warn"
    }
}